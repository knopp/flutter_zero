// VSCode workspace settings that are shared among all users of this project.
// This only affects subdirectories of this project.
{
    "html.format.enable": false,
    "githubPullRequests.ignoredPullRequestBranches": [
        "master"
    ],
    "files.trimTrailingWhitespace": true,
    "lldb-dap.postRunCommands": [
        "settings set target.source-map flutter /Users/Matej/Projects/flutter_minimal/engine/src/flutter",
    ],
    "java.import.gradle.enabled": false,
    "java.import.maven.enabled": false,
    // "java.configuration.updateBuildConfiguration": "automatic",
    "java.project.sourcePaths": [
        "engine/src/flutter/shell/platform/android",
        "engine/src/flutter/shell/platform/android/test"
    ],
    "java.project.referencedLibraries": [
        "engine/src/flutter/third_party/android_tools/sdk/platforms/android-36/android.jar",
        "engine/src/flutter/third_party/android_embedding_dependencies/lib/*.jar",
        "engine/src/flutter/third_party/android_embedding_test_dependencies/lib/*.jar",
    ],
    "dart.flutterSdkPath": "."
}