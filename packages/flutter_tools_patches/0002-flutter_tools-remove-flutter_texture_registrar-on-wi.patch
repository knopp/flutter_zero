From: Matej Knopp <matej.knopp@gmail.com>
Date: Tue, 30 Dec 2025 19:39:17 +0100
Subject: [PATCH 29/37] flutter_tools: remove flutter_texture_registrar on
 windows

---
 .../flutter_tools/lib/src/build_system/targets/windows.dart     | 1 -
 .../templates/app/windows.tmpl/flutter/CMakeLists.txt           | 2 --
 .../test/general.shard/build_system/targets/windows_test.dart   | 1 -
 3 files changed, 4 deletions(-)

diff --git a/packages/flutter_tools/lib/src/build_system/targets/windows.dart b/packages/flutter_tools/lib/src/build_system/targets/windows.dart
index a6e2e4b698..d44005cf48 100644
--- a/packages/flutter_tools/lib/src/build_system/targets/windows.dart
+++ b/packages/flutter_tools/lib/src/build_system/targets/windows.dart
@@ -25,7 +25,6 @@ const _kWindowsArtifacts = <String>[
   'flutter_export.h',
   'flutter_messenger.h',
   'flutter_plugin_registrar.h',
-  'flutter_texture_registrar.h',
   'flutter_windows.h',
 ];
 
diff --git a/packages/flutter_tools/templates/app/windows.tmpl/flutter/CMakeLists.txt b/packages/flutter_tools/templates/app/windows.tmpl/flutter/CMakeLists.txt
index 903f4899d6..41cb9ff628 100644
--- a/packages/flutter_tools/templates/app/windows.tmpl/flutter/CMakeLists.txt
+++ b/packages/flutter_tools/templates/app/windows.tmpl/flutter/CMakeLists.txt
@@ -29,7 +29,6 @@ list(APPEND FLUTTER_LIBRARY_HEADERS
   "flutter_windows.h"
   "flutter_messenger.h"
   "flutter_plugin_registrar.h"
-  "flutter_texture_registrar.h"
 )
 list(TRANSFORM FLUTTER_LIBRARY_HEADERS PREPEND "${EPHEMERAL_DIR}/")
 add_library(flutter INTERFACE)
@@ -51,7 +50,6 @@ list(APPEND CPP_WRAPPER_SOURCES_PLUGIN
 list(TRANSFORM CPP_WRAPPER_SOURCES_PLUGIN PREPEND "${WRAPPER_ROOT}/")
 list(APPEND CPP_WRAPPER_SOURCES_APP
   "flutter_engine.cc"
-  "flutter_view_controller.cc"
 )
 list(TRANSFORM CPP_WRAPPER_SOURCES_APP PREPEND "${WRAPPER_ROOT}/")
 
diff --git a/packages/flutter_tools/test/general.shard/build_system/targets/windows_test.dart b/packages/flutter_tools/test/general.shard/build_system/targets/windows_test.dart
index 020325eb72..d7c25cae8e 100644
--- a/packages/flutter_tools/test/general.shard/build_system/targets/windows_test.dart
+++ b/packages/flutter_tools/test/general.shard/build_system/targets/windows_test.dart
@@ -53,7 +53,6 @@ void main() {
         '$windowsDesktopPath\\flutter_windows.dll.lib',
         '$windowsDesktopPath\\flutter_windows.dll.pdb',
         '$windowsDesktopPath\\flutter_plugin_registrar.h',
-        '$windowsDesktopPath\\flutter_texture_registrar.h',
         '$windowsDesktopPath\\flutter_windows.h',
         icuData,
         '$windowsCppClientWrapper\\foo',
